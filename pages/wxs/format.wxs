
var formatDateTime = function (utcDate) {
  if (typeof utcDate != 'string') return '';
  const strs = utcDate.split('T');
  const date = strs[0];
  const _time = strs[1];
  const time = _time.split('.')[0];
  return date + " " + time;
}
