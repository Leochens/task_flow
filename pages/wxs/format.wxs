var formatTime = function (utcDate) {
  if (typeof utcDate != 'string') return '';
  const strs = utcDate.split('T');
  const _time = strs[1];
  const time = _time.split('.')[0];
  return time;
}
module.exports = {
  formatDate: formatDate,
  formatDateTime: formatDateTime,
  formatTime: formatTime
}
